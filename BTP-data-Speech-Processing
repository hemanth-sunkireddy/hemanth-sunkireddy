    "import os\n",
    "import zipfile\n",
    "from tqdm import tqdm  # for progress bar\n",
    "\n",
    "# Folder to zip\n",
    "folder_path = \"/kaggle/working/extracted_text\"\n",
    "zip_path = \"/kaggle/working/extracted_text.zip\"\n",
    "\n",
    "print(\"Started ZIP\")\n",
    "# Get all file paths in the folder\n",
    "file_paths = []\n",
    "for root, dirs, files in os.walk(folder_path):\n",
    "    for file in files:\n",
    "        full_path = os.path.join(root, file)\n",
    "        file_paths.append(full_path)\n",
    "\n",
    "print(f\"Total files to zip: {len(file_paths)}\")\n",
    "\n",
    "# Create zip with progress bar\n",
    "with zipfile.ZipFile(zip_path, 'w', zipfile.ZIP_DEFLATED) as zipf:\n",
    "    for file in tqdm(file_paths, desc=\"Zipping files\"):\n",
    "        # Add file to zip preserving folder structure\n",
    "        arcname = os.path.relpath(file, folder_path)\n",
    "        zipf.write(file, arcname)\n",
    "\n",
    "print(f\"✅ ZIP created at: {zip_path}\")\n"
   ]
  }
